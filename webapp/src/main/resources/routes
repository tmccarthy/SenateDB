# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           au.id.tmm.senatedb.webapp.controllers.HomeController.index

GET     /division/:electionId/:state/:divisionName       au.id.tmm.senatedb.webapp.controllers.DivisionController.index(electionId: String, state: String, divisionName: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# TODO make this restful
GET     /population/:electionId/status         au.id.tmm.senatedb.webapp.controllers.DbPopulationController.checkPopulationStatusFor(electionId: String)
GET     /population/:electionId/populate       au.id.tmm.senatedb.webapp.controllers.DbPopulationController.populateFor(electionId: String)